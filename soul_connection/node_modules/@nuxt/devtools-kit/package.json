{
  "name": "@nuxt/devtools-kit",
  "type": "module",
  "version": "1.4.1",
  "license": "MIT",
  "homepage": "https://devtools.nuxt.com/module/utils-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/devtools.git",
    "directory": "packages/devtools-kit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./iframe-client": {
      "types": "./iframe-client.d.ts",
      "import": "./iframe-client.mjs"
    },
    "./host-client": {
      "types": "./host-client.d.ts",
      "import": "./host-client.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "*.cjs",
    "*.d.ts",
    "*.mjs",
    "dist"
  ],
  "peerDependencies": {
    "vite": "*"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.0",
    "@nuxt/schema": "^3.13.0",
    "execa": "^7.2.0"
  },
  "devDependencies": {
    "birpc": "^0.2.17",
    "error-stack-parser-es": "^0.1.5",
    "hookable": "^5.5.3",
    "unbuild": "^2.0.0",
    "unimport": "^3.11.1",
    "vite-plugin-vue-inspector": "^5.1.3",
    "vue-router": "^4.4.3"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev:prepare": "nr stub"
  }
}